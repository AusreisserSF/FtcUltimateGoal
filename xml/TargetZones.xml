  </OpMode>
  <OpMode id="BLUE_INSIDE">
    <opmode_target_zones>
      <TARGET_ZONE_A>
      </TARGET_ZONE_A>
      <TARGET_ZONE_B>
      </TARGET_ZONE_B>
      <TARGET_ZONE_C>
      </TARGET_ZONE_C>
    </opmode_target_zones>
  </OpMode>
  <OpMode id="BLUE_OUTSIDE">
    <opmode_target_zones>
      <TARGET_ZONE_A>
      <MOVE>
            <distance>62.0</distance>
            <margin>1.0</margin>
            <power>.75</power>
            <direction>180.0</direction>
            <heading>0.0</heading>
            <pid>
                <p>0.0</p>
                <i>0.0</i>
                <d>0.0</d>
            </pid>
        </MOVE>
        
        <SLEEP>
            <ms>1000</ms>
        </SLEEP>

        <MOVE>
            <distance>12.0</distance>
            <margin>1.0</margin>
            <power>.75</power>
            <direction>0</direction>
            <heading>0.0</heading>
            <pid>
                <p>0.0</p>
                <i>0.0</i>
                <d>0.0</d>
            </pid>
        </MOVE>

        <TURN>
            <angle>0</angle>
            <minpower>0.2</minpower>
            <maxpower>1</maxpower>
            <margin>3</margin>
            <pid>
                <p>0.031415926 </p>
                <i>0.0</i>
                <d>0.0</d>
            </pid>
        </TURN>

        <SHOOT>
            <shootVelocity>1500</shootVelocity>
            <waitTime>3000</waitTime>
            
        </SHOOT>

        <MOVE>
            <distance>10.0</distance>
            <margin>1.0</margin>
            <power>.75</power>
            <direction>180</direction>
            <heading>0.0</heading>
            <pid>
                <p>0.0</p>
                <i>0.0</i>
                <d>0.0</d>
            </pid>
        </MOVE>

      </TARGET_ZONE_A>
      <TARGET_ZONE_B></TARGET_ZONE_B>
      <TARGET_ZONE_C></TARGET_ZONE_C>
    </opmode_target_zones>
  </OpMode>
  <OpMode id="RED_INSIDE">
