<RobotConfig>

  <WEBCAM>
      <present>yes</present>
  </WEBCAM>

  <!-- servos -->
  <TRIGGER>
    <rest>0.0</rest>
    <out>0.15</out>
  </TRIGGER>
  <WOBBLE>
    <rest>0.4</rest>
    <hold>1</hold> <!-- changed from 0.90 harold-->
    <release>0.25</release> 
  </WOBBLE>

  <!-- motors -->
  <ELEVATOR>
    <power>0.6</power>
    <up>-400</up>
    <down>0</down>
  </ELEVATOR>

  <!-- teleop -->
  <FULL_DRIVE>
    <powershot_high>2300</powershot_high>
    <powershot_low>1800</powershot_low>
    <high_goal>2000</high_goal>
    <intake>3000</intake>
  </FULL_DRIVE>

</RobotConfig>